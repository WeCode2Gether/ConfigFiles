{
   "action": {
      "default_popup": "popup.html"
   },
   "background": {
      "service_worker": "background.js",
      "type": "module"
   },
   "commands": {
      "_execute_action": {
         "description": "__MSG_commandOpenClipper__",
         "suggested_key": {
            "default": "Ctrl+Shift+O",
            "mac": "Command+Shift+O"
         }
      },
      "quick_clip": {
         "description": "__MSG_commandQuickClip__",
         "suggested_key": {
            "default": "Alt+Shift+O",
            "mac": "Alt+Shift+O"
         }
      },
      "toggle_highlighter": {
         "description": "__MSG_commandToggleHighlighter__",
         "suggested_key": {
            "default": "Alt+Shift+H",
            "mac": "Alt+Shift+H"
         }
      }
   },
   "content_scripts": [ {
      "css": [ "highlighter.css" ],
      "js": [ "content.js" ],
      "matches": [ "http://*/*", "https://*/*" ]
   } ],
   "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
   },
   "default_locale": "en",
   "description": "Save content from the web in a private and durable format that you can access offline. The official browser extension for Obsidian.",
   "homepage_url": "https://obsidian.md/",
   "host_permissions": [ "\u003Call_urls>", "http://*/*", "https://*/*" ],
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Obsidian Web Clipper",
   "options_ui": {
      "open_in_tab": true,
      "page": "settings.html"
   },
   "permissions": [ "activeTab", "clipboardWrite", "commands", "contextMenus", "sidePanel", "storage", "scripting" ],
   "side_panel": {
      "default_path": "side-panel.html"
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.10.7"
}
